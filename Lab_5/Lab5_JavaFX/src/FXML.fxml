<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="titleLabel" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
   <children>
      <Label fx:id="titleRegisterLabel" layoutX="239.0" layoutY="21.0" prefHeight="32.0" prefWidth="102.0" text="Registration">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="nameLabel" layoutX="110.0" layoutY="84.0" text="Name" />
      <TextField fx:id="nameTextField" layoutX="225.0" layoutY="80.0" promptText="Name" />
      <Button fx:id="submitButton" layoutX="259.0" layoutY="240.0" mnemonicParsing="false" onAction="#submit" text="Submit" />
      <Label fx:id="ageLabel" layoutX="112.0" layoutY="137.0" text="Age" />
      <TextField fx:id="ageTextField" layoutX="225.0" layoutY="133.0" promptText="Age" />
      <Label layoutX="109.0" layoutY="200.0" text="Email" />
      <TextField fx:id="emailTextField" layoutX="226.0" layoutY="196.0" promptText="Email" />
   </children>
</AnchorPane>
